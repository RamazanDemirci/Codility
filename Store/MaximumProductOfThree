#include <iostream>
#include <math.h>
#include <vector>
#include <algorithm>
#include <numeric>
#include <climits>
#include <iterator>

using namespace std;
//MaximumProductOfThree

int solution(vector<int> &vec){
    int res = 0;
    sort(vec.begin(), vec.end());

    int P1 = 0, P2 = 0;
    int N = vec.size();
    P1 = vec[N-1]*vec[0]*vec[1];
    P2 = vec[N-1]*vec[N-2]*vec[N-3];
    res = max(P1, P2);
    return res;
}

int main() {
    std::cout << "Hello Easy C++ project!" << std::endl;
    vector<int> P{-3,1,2,-2,5,6};
    auto res = solution(P);

    std::cout << "\nres : " << res;
}